<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.charlesdelmaire.qingpool"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.GET_ACCOUNTS" />
	<uses-permission android:name="android.permission.USE_CREDENTIALS" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:debuggable="true">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>
        <activity
            android:name="net.charlesdelmaire.qingpool.accueilActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
        </activity>
         <activity 
        	android:name="connexionActivity"
        	android:label="Connexion">
        </activity>
        <activity 
        	android:name="principaleActivity"
        	android:label="Page principale">
        </activity>
        <activity 
        	android:name="nouveauPoolActivity"
        	android:label="Création d'un pool">
        </activity>
        <activity 
        	android:name="connPoolActivity"
        	android:label="Connexion à un pool">
        </activity>
        <activity 
        	android:name="listJoueActivity"
        	android:label="Liste des joueurs d'un participant">
        </activity>
        <activity 
        	android:name="listPartiActivity"
        	android:label="Liste des participants">
        </activity>
        <activity 
        	android:name="listPoolActivity"
        	android:label="Liste des pools d'un participant">
        </activity>
        <activity 
        	android:name="listRandActivity"
        	android:label="Liste de joueur aléatoire">
        </activity>
        <activity 
        	android:name="rsltFinalActivity"
        	android:label="Résultats finaux d'un pool">
        </activity>
        <activity 
        	android:name="choixJoueActivity"
        	android:label="Choix des joueurs">
        </activity>
        
    </application>

</manifest>
